{
    "name": "eslint-config-koot",
    "version": "0.2.0",
    "description": "ESLint configuration used by Koot.js & its boilerplate",
    "author": {
        "name": "diablohu",
        "email": "diablohudream@gmail.com",
        "url": "https://diablohu.com"
    },
    "contributors": [
        {
            "name": "daqi",
            "url": "https://github.com/daqi"
        }
    ],
    "homepage": "https://github.com/cmux/eslint-config-koot#readme",
    "license": "Apache-2.0",
    "repository": {
        "type": "git",
        "url": "https://github.com/cmux/eslint-config-koot.git"
    },
    "keywords": [
        "eslint",
        "eslintconfig"
    ],
    "scripts": {},
    "dependencies": {
        "@typescript-eslint/eslint-plugin": "1.11.0",
        "@typescript-eslint/parser": "1.11.0",
        "babel-eslint": "10.0.2",
        "eslint-config-prettier": "5.0.0",
        "eslint-config-react-app": "4.0.1",
        "eslint-plugin-flowtype": "3.10.5",
        "eslint-plugin-import": "2.17.3",
        "eslint-plugin-jsx-a11y": "6.2.1",
        "eslint-plugin-prettier": "3.1.0",
        "eslint-plugin-react": "7.14.0",
        "eslint-plugin-react-hooks": "1.6.0"
    },
    "devDependencies": {
        "eslint": "5.16.0",
        "husky": "2.4.1",
        "lint-staged": "8.2.1",
        "prettier": "1.18.2"
    },
    "peerDependencies": {
        "eslint": ">=5"
    },
    "bugs": {
        "url": "https://github.com/cmux/eslint-config-koot"
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged"
        }
    },
    "lint-staged": {
        "*.{js,jsx,ts,tsx,cjs,mjs}": [
            "eslint --fix",
            "prettier --write",
            "git add"
        ],
        "*.{json,md,css,less,sass,scss}": [
            "prettier --write",
            "git add"
        ]
    }
}
